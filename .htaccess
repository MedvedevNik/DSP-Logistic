
Options All -ExecCGI -Indexes -Includes +FollowSymLinks
Options -MultiViews

AddDefaultCharset UTF-8
AddCharset utf-8 .css .js .txt .json .rss .xml


ServerSignature Off

ServerSignature EMail
SetEnv SERVER_ADMIN dsplogistic@mail.ru

<IfModule mod_rewrite.c>
 RewriteEngine On
 RewriteBase /
</IfModule>


<IfModule mod_rewrite.c>
 RewriteCond %{HTTP_HOST} ^www\.(.+)$
 RewriteRule ^(.*)$ https://%{HTTP_HOST}%{REQUEST_URI} [R=301,L]
RewriteCond %{HTTP:X-Forwarded-Proto} =http
RewriteRule ^(.*)$ https://%{HTTP_HOST}%{REQUEST_URI} [R=301,L]
</IfModule>

<IfModule mod_rewrite.c>
 RewriteCond %{REQUEST_URI} ^(.*)/{2,}(.*)$
 RewriteRule ^(.*)$ %1/%2 [R=301,L]
 RewriteCond %{THE_REQUEST} ([^\s]*)\/{2,}(\?[^\s]*)?
 RewriteRule ^(.*)$ / [R=301,L]
</IfModule>

<IfModule mod_rewrite.c>
 RewriteCond %{THE_REQUEST} ^[A-Z]{3,9}\ /([^/]+/)*(default|index|main)\.(html|php|htm)\ HTTP/ [NC]
 RewriteRule ^(([^/]+/)*)(default|main|index)\.(html|php|htm)$ https://%{SERVER_NAME}/$1 [R=301,L]
</IfModule>


<IfModule mod_dir.c>
 DirectoryIndex index.php index.html
</IfModule>


ErrorDocument 400 /error/400.html

ErrorDocument 401 /error/401.html

ErrorDocument 403 /error/403.html
ErrorDocument 404 /error/404.html
ErrorDocument 500 /error/500.html
ErrorDocument 502 /error/502.html
ErrorDocument 503 /error/503.html
ErrorDocument 504 /error/504.html
ErrorDocument 505 "HTTP Version not supported. Версия HTTP не поддерживается."

<IfModule mod_mime.c>
 AddHandler application/x-httpd-php .html
