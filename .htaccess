<IfModule mod_headers.c>
 <FilesMatch "\.(php)$">
 Header set Cache-Control "max-age=15"
 </FilesMatch>
 <FilesMatch "\.(html|htm|xml)$"> 
 Header set Cache-Control "max-age=1800"
 </FilesMatch>
 <FilesMatch "\.(js|css|txt)$"> 
 Header set Cache-Control "max-age=604800"
 </FilesMatch>
  <FilesMatch "\.(ico)$">
  Header set Cache-Control "max-age=2592000"
  </FilesMatch>
 <FilesMatch "\.(flv|swf|ttf|otf|woff|gif|png|jpg|jpe?g|doc?x|xls?x|mov|avi|pdf|rar|zip|mp3|wmv|ppt|tar|gz|svg?z)$">
 Header set Cache-Control "max-age=31536000"
 </FilesMatch>
 <FilesMatch "\.(json|pl|cgi|spl|scgi|fcgi)$">
 Header unset Cache-Control
 </FilesMatch>
 <FilesMatch "\.(ttf|ttc|otf|eot|woff|font.css)$"> 
 Header set Access-Control-Allow-Origin "*"
 </FilesMatch>
 <FilesMatch "\.(js|css|gif|png|jpg|jpe?g|doc?x|xls?x|mov|avi|pdf|rar|zip|mp3|wmv|ppt|tar|gz|xml|oga|ogg|m4a|ogv|mp4|m4v|webm|svg?z|eot|ttf|otf|woff|ico|webp|appcache|manifest|htc|crx|oex|xpi|safariextz|vcf)$"> 
 Header unset X-UA-Compatible
 </FilesMatch>
</IfModule>
